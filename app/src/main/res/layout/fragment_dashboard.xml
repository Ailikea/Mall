<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--    静态页面   -->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageButton
            android:id="@+id/image_Button"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="9dp"
            android:layout_marginLeft="11dp"
            android:layout_marginTop="30dp"
            android:layout_alignParentStart="true"
            android:src="@mipmap/icon_duihuanjilu"
            android:layout_alignParentLeft="true" />
        <LinearLayout
            android:orientation="vertical"
            android:layout_marginTop="50dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@id/image_Button">



            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="手机号" />
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="130dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linear1"
            style="@style/edit_profile_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/roll"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="65dp"
                android:layout_marginLeft="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/Pangolin_button"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/chuanshanjia" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/accessibility_custom_action_11"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/tengxunguanggao" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/iv2"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/baidu" />
            </LinearLayout>

        </LinearLayout>


        <RelativeLayout
            android:id="@+id/sigmob_Button"
            style="@style/edit_profile_group"
            android:background="@drawable/ic_dashboard_btn_chat_charge"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"

                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text=" sigmob视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/sigmob_logo" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="0dp"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/qiangbao2"
            style="@style/edit_profile_group"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text="vungle视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/vungle" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="2dp"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/Huawei_button"
            style="@style/edit_profile_group"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text=" 华为视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/huawei" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="2dp"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/qiangbao"
            style="@style/edit_profile_group"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text=" 快手视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/kuiashou" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="2dp"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/qiangbaoa"
            style="@style/edit_profile_group"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text=" 万唯视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/wanwei" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="0dp"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/Rel_Mintegral"
            style="@style/edit_profile_group"
            android:onClick="editProfileClick"
            tools:ignore="OnClick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="230dp"
                android:layout_marginRight="25dp"
                android:ellipsize="end"
                android:gravity="right"
                android:singleLine="true"
                android:text=" mintegral视频"
                android:textColor="#ff333333"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="101dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/mintegral" />

            <ImageView
                android:layout_width="16dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_arrow_right" />
        </RelativeLayout>


    </LinearLayout>

</RelativeLayout>